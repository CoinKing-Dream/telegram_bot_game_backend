{
    "version": 2,
    "builds": [
      {
        "src": "src/server.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/$1"
      }
    ],
    "env": {
      "DATABASE_URL": "@env:DATABASE_URL",
      "API_KEY": "@env:API_KEY"
    }
  }
